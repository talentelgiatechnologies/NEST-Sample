{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/core/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/core/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/core",
  "projects": {
    "core": {
      "type": "application",
      "root": "apps/core",
      "entryFile": "main",
      "sourceRoot": "apps/core/src",
      "compilerOptions": {
        "tsConfigPath": "apps/core/tsconfig.app.json"
      }
    },
    "upload": {
      "type": "application",
      "root": "apps/upload",
      "entryFile": "main",
      "sourceRoot": "apps/upload/src",
      "compilerOptions": {
        "tsConfigPath": "apps/upload/tsconfig.app.json"
      }
    },
    "mobile": {
      "type": "application",
      "root": "apps/mobile",
      "entryFile": "main",
      "sourceRoot": "apps/mobile/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mobile/tsconfig.app.json"
      }
    },
    "cron": {
      "type": "application",
      "root": "apps/cron",
      "entryFile": "main",
      "sourceRoot": "apps/cron/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cron/tsconfig.app.json"
      }
    },
    "experience": {
      "type": "application",
      "root": "apps/experience",
      "entryFile": "main",
      "sourceRoot": "apps/experience/src",
      "compilerOptions": {
        "tsConfigPath": "apps/experience/tsconfig.app.json"
      }
    }
  }
}